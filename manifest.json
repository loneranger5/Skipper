{
  "name": "Skipper",
  "description": "Simple Javascript plugin",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [  {
    "matches": [
      
      "https://www.oneplus.in/*",
      "https://storeapi.oneplus.in/",
      "https://accenture.percipio.com/*"
    ],
    "js": ["content.js"]
  }
  
],
"background": {
  "service_worker": "background.js"
},
"action": {
    
    "default_icon": {
      "48": "icon.png"
    }
},


"host_permissions": [
  "https://www.oneplus.in/*",
  "https://storeapi.oneplus.in/*",
  "https://accenture.percipio.com/"
],

"permissions":[
  "storage"

]


}